<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<Accordion fx:id="sidePaneAccordion" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfxmessenger.SidePaneAccordionController">
  <panes>
    <TitledPane animated="false" text="Channels">
      <content>
        <AnchorPane >
               <children>
                  <ListView AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
        </AnchorPane>
      </content>
    </TitledPane>
    <TitledPane animated="false" text="Contacts">
      <content>
        <AnchorPane >
               <children>
                  <ListView fx:id="contactListView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
        </AnchorPane>
      </content>
    </TitledPane>
  </panes>
</Accordion>
